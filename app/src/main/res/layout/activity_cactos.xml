<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Cactos"
    android:orientation="vertical"
    android:background="@color/white"
    >


<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cactos"
    android:background="@color/white">

    <ImageView
        android:id="@+id/logo1"
        style="@style/Logo1"
        android:src="@drawable/gaiagardenlogo"


        android:layout_marginRight="282dp"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/barra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <View
        android:id="@+id/barra"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/verde"

        android:layout_marginTop="40dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/cacto2"
        />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/perfil"
        android:src="@drawable/perfil"

        android:layout_height="35dp"
        android:layout_width="35dp"
        android:layout_marginRight="360dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/logo1"
        app:layout_constraintStart_toStartOf="@id/barra"
        app:layout_constraintBottom_toBottomOf="@id/barra"
        />

    <TextView
        android:id="@+id/perfiltexto"
        android:text="Perfil"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"

        app:layout_constraintBottom_toBottomOf="@id/barra"
        app:layout_constraintTop_toTopOf="@id/barra"
        app:layout_constraintStart_toEndOf="@id/perfil"
        />

    <ImageView
        android:id="@id/imgfalecnsc"
        android:layout_width="35dp"
        android:layout_height="35dp"

        android:layout_marginRight="20dp"

        android:src="@drawable/faleconosco"

        app:layout_constraintTop_toTopOf="@id/barra"
        app:layout_constraintStart_toEndOf="@id/perfiltexto"
        app:layout_constraintEnd_toEndOf="@id/barra"
        app:layout_constraintBottom_toBottomOf="@id/barra"
        />

    <TextView
        android:id="@+id/faleconosco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="20sp"

        android:layout_marginRight="5dp"

        android:textColor="@color/white"
        android:textStyle="bold"

        android:text="Fale Conosco"

        app:layout_constraintStart_toEndOf="@id/imgfalecnsc"
        app:layout_constraintEnd_toEndOf="@id/barra"
        app:layout_constraintBottom_toBottomOf="@id/barra"
        app:layout_constraintTop_toTopOf="@id/barra"
        />

    <ImageView
        android:id="@+id/cactobola"
        android:layout_width="100dp"
        android:layout_height="100dp"

        android:src="@drawable/cactobola"

        android:layout_marginRight="253dp"
        android:layout_marginTop="180dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/cactolua"
        />

    <TextView
        android:id="@+id/textocactobola"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cacto Bola"

        android:textStyle="bold"
        android:textColor="@color/texto"
        android:layout_marginRight="255dp"

        android:textSize="15sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cactobola"
        />

    <TextView
        android:id="@+id/precocactobola"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="R$ 23,58"

        android:textStyle="bold"
        android:textColor="@color/texto"
        android:layout_marginRight="253dp"

        android:textSize="15sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textocactobola"
        />

    <ImageView
        android:id="@+id/cactolua"
        android:layout_width="100dp"
        android:layout_height="100dp"

