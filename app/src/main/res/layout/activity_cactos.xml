        android:layout_marginBottom="60dp"
        android:layout_marginRight="255dp"

        android:textSize="15sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cactolua"
        />

    <TextView
        android:id="@+id/precolua"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="R$ 20,00"

        android:textStyle="bold"
        android:textColor="@color/texto"
        android:layout_marginRight="253dp"
        android:layout_marginBottom="40dp"

        android:textSize="15sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textolua"
        />

    <ImageView
        android:id="@+id/nivosa"
        android:layout_width="100dp"
        android:layout_height="100dp"

        android:src="@drawable/cactoparodianivosa"
        android:layout_marginRight="253dp"
        android:layout_marginTop="50dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cactolua"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/textonivosa"
